{
  "name": "nuxt-typed-router",
  "version": "0.2.0",
  "description": "Provide autocompletion for pages route names generated by Nuxt router",
  "main": "lib/module.js",
  "author": "Victor Garcia <victorgarciaparis13@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nuxt typed router",
    "nuxt router",
    "nuxt typed",
    "nuxt safe router",
    "nuxt typed routes",
    "nuxt generate route ts"
  ],
  "scripts": {
    "dev": "tsc -p ./tsconfig.json --pretty --watch",
    "build": "tsc -p ./tsconfig.json --pretty",
    "prepublish": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "types/index.d.ts",
    "types/vue.d.ts"
  ],
  "typings": "types/index.d.ts",
  "dependencies": {
    "chalk": "^4.1.0",
    "lodash": "^4.17.20",
    "log-symbols": "^4.0.0",
    "prettier": "^2.2.0"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.7",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.10",
    "@types/prettier": "^2.1.5",
    "typescript": "^4.1.2",
    "vue-router": "^3.4.9"
  }
}
