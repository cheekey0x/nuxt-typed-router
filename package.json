{
  "name": "nuxt-typed-router",
  "version": "1.0.0-alpha-2",
  "description": "Provide autocompletion for pages route names generated by Nuxt router",
  "main": "lib/module.js",
  "keywords": [
    "nuxt typed router",
    "nuxt router",
    "nuxt typed",
    "nuxt safe router",
    "nuxt typed routes",
    "nuxt generate route ts"
  ],
  "scripts": {
    "dev": "tsc -p ./tsconfig.json --pretty --watch",
    "build": "rimraf lib && rimraf types && tsc -p ./tsconfig.json --pretty",
    "copy-files": "copyfiles -u 1 ./src/**/*.js ./lib",
    "prepublish": "yarn build",
    "test": "jest",
    "test:reset": "yarn test --updateSnapshot"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "types/index.d.ts",
    "types/types.d.ts",
    "types/vue.d.ts",
    "types/hooks.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://victorgarciaesgi@github.com/victorgarciaesgi/nuxt-typed-router.git"
  },
  "author": {
    "name": "Victor Garcia",
    "url": "https://github.com/victorgarciaesgi"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victorgarciaesgi/nuxt-typed-router/issues"
  },
  "typings": "types/index.d.ts",
  "dependencies": {
    "chalk": "^4.1.2",
    "lodash": "^4.17.21",
    "log-symbols": "^5.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@nuxt/kit": "^0.8.1-edge",
    "@nuxt/test-utils": "^0.2.2",
    "@nuxt/types": "^2.15.8",
    "jest": "^27.4.3",
    "prettier": "^2.5.1",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.12",
    "@types/prettier": "^2.4.2",
    "babel-jest": "^27.4.2",
    "copyfiles": "^2.4.0",
    "core-js": "^3.19.3",
    "nuxt": "^2.15.8",
    "playwright": "^1.17.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.2",
    "vue-router": "^4.0.12"
  }
}
