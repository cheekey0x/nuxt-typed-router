{
  "name": "nuxt-typed-router",
  "version": "1.0.0-beta-12",
  "description": "Provide autocompletion for pages route names generated by Nuxt router",
  "scripts": {
    "dev": "tsc -p ./tsconfig.json --pretty --watch",
    "build": "rimraf dist && unbuild",
    "buildts": "rimraf lib && rimraf types && tsc -p ./tsconfig.json --pretty && mkdir ./lib/templates && cp ./src/templates/typed-router.js ./lib/templates/typed-router.js",
    "prepublish": "yarn build",
    "test": "jest",
    "test:reset": "yarn test --updateSnapshot"
  },
  "type": "module",
  "main": "./dist/module.mjs",
  "exports": {
    ".": "./dist/module.mjs"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/module.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nuxt typed router",
    "nuxt router",
    "nuxt typed",
    "nuxt safe router",
    "nuxt typed routes",
    "nuxt generate route ts",
    "nuxt 3",
    "nuxt 3 router"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://victorgarciaesgi@github.com/victorgarciaesgi/nuxt-typed-router.git"
  },
  "author": {
    "name": "Victor Garcia",
    "url": "https://github.com/victorgarciaesgi"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victorgarciaesgi/nuxt-typed-router/issues"
  },
  "dependencies": {
    "@nuxt/kit": "^0.8.1-edge",
    "chalk": "^4.1.2",
    "lodash": "^4.17.21",
    "log-symbols": "^5.1.0",
    "mkdirp": "^1.0.4",
    "pathe": "^0.2.0",
    "prettier": "^2.5.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@nuxt/test-utils": "^0.2.2",
    "@nuxt/types": "^2.15.8",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.11.12",
    "@types/prettier": "^2.4.2",
    "babel-jest": "^27.4.4",
    "copyfiles": "^2.4.0",
    "core-js": "^3.19.3",
    "jest": "^27.4.4",
    "nuxt3": "^3.0.0-27319101.3e82f0f",
    "path": "^0.12.7",
    "playwright": "^1.17.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "typescript": "^4.6.0-dev.20211216",
    "unbuild": "^0.6.7",
    "vue-router": "^4.0.12"
  }
}
