{
  "extends": ["@nuxtjs/eslint-config-typescript", "plugin:vue/vue3-recommended", "prettier"],
  "rules": {
    "semi": "off",
    "prefer-const": "off",
    "no-console": "off",
    "vue/multi-word-component-names": "off",
    "@typescript-eslint/no-unused-vars": ["off"],
    "import/default": "off",
    "import/no-named-as-default-member": "off"
  }
}
